{
    "source": {
        "include": ["./"],
        "includePattern": ".js$",
        "excludePattern": "node_modules|docs"
    },
    "sourceType": "module",
    "plugins": ["plugins/markdown"], 
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true
    }
}
